


.signinbtn{
	float: right;
	background-color: #4CAF50; /* Green */
	border: none;
	padding: 6px 18px;
	text-align: center;
	display: inline-block;
	font-size: 18px;
	text-decoration: none;
	color: red;
	margin-top: 18px;
	margin-right: 24px;
}

.signupbtn{
	float: right;
